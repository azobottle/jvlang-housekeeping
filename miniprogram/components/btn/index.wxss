/* components/btn/index.wxss */
.btn {
  overflow: hidden;
  border: rgb(24, 49, 83) solid 2px;
  border-radius: 10px;
  box-shadow: rgb(24, 49, 83) 0px 5.74219px 0px 0px;
  font-size: 15px;
  padding: 14px 9px;
  cursor: pointer;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  letter-spacing: 1px;
}