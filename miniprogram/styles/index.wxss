.rows {
  display: flex;
  flex-direction: column;
  height: 100%;
}

.columns {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.height100 {
  height: 100%;
}

.flex_center {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.fade_in {
  animation: fadeIn 0.6s;
}

@keyframes fadeIn {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}